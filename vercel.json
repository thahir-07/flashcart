{
  "version": 2,
  "builds": [
    {
      "src": "./bin/www",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "./bin/www"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "./bin/www"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
